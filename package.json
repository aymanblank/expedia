{
  "name": "expedia",
  "version": "1.0.0",
  "description": "Expedia Coding Exercise",
  "main": "server.js",
  "scripts": {
    "client": "react-scripts start",
    "build": "react-scripts build",
    "server": "node server.js",
    "dev-server": "nodemon server.js",
    "start-dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint src/**",
    "lint:watch": "esw -w src/**",
    "review": "npm run lint && npm test",
    "start": "npm run build && NODE_ENV=production node server.js",
    "test": "jest src/**",
    "test:watch": "jest src/** --watch",
    "test:watchAll": "jest src/** --watchAll",
    "test:coverage:report": "open coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aymanblank/expedia.git"
  },
  "keywords": [
    "Expedia"
  ],
  "author": "Ayman Abu Khalifa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aymanblank/expedia/issues"
  },
  "homepage": "https://polar-dawn-15847.herokuapp.com/",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "enzyme-adapter-react-16": "^1.0.0",
    "express": "^4.16.2",
    "material-ui": "^0.20.0",
    "node-sass": "^4.5.3",
    "prop-types": "^15.6.0",
    "raf": "^3.3.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0"
  },
  "devDependencies": {
    "nodemon": "^1.15.0",
    "babel-eslint": "^8.0.1",
    "enzyme": "^3.0.0",
    "eslint": "^4.7.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.2",
    "jest": "^21.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-hot-middleware": "^2.19.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleDirectories": [
      "node_modules",
      "./"
    ],
    "moduleNameMapper": {
      "^.+.(css|scss|sass)$": "<rootDir>/__tests__/cssStub.js"
    },
    "setupFiles": [
      "raf/polyfill"
    ]
  }
}
