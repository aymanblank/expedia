{
  "name": "expedia",
  "version": "1.0.0",
  "description": "Expedia Coding Exercise",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "server": "node server.js",
    "dev-server": "nodemon server.js",
    "client": "npm start --prefix client",
    "init": "concurrently \"npm install\" \"npm run client-install\"",
    "dev": "concurrently \"npm run dev-server\" \"npm run client\"",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aymanblank/expedia.git"
  },
  "keywords": [
    "Expedia"
  ],
  "author": "Ayman Abu Khalifa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aymanblank/expedia/issues"
  },
  "homepage": "https://github.com/aymanblank/expedia#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "nodemon": "^1.15.0"
  }
}
